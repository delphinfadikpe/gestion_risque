{% extends 'base.html.twig' %}

{% block title %}StatutPerte index{% endblock %}

{% block body %}
    <h1>StatutPerte index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Libelle</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for statut_perte in statut_pertes %}
            <tr>
                <td>{{ statut_perte.id }}</td>
                <td>{{ statut_perte.libelle }}</td>
                <td>
                    <a href="{{ path('app_statut_perte_show', {'id': statut_perte.id}) }}">show</a>
                    <a href="{{ path('app_statut_perte_edit', {'id': statut_perte.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_statut_perte_new') }}">Create new</a>
{% endblock %}
